{{#each time as |timeRecord|}}
<div class="col-sm-6">
<div class="card" style="width: 18rem;">
    <div class="card-body">
    <h5 class="card-title">Employee number: {{timeRecord.employeeID}}</h5>
    <p class="card-text">Job number:{{timeRecord.jobID}} = {{timeRecord.Hours_worked}}hours</p>
    </div>
</div>
</div>
{{/each}}

